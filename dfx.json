{
  "dfx": "0.14.1-beta.1",
  "canisters": {
    "bitcoin": {
      "type": "rust",
      "candid": "./canister/candid.did",
      "package": "ic-btc-canister",
      "optimize": "cycles",
      "gzip": true,
      "pullable": {
        "wasm_url": "https://github.com/dfinity/bitcoin-canister/releases/latest/download/ic-btc-canister.wasm.gz",
        "dependencies": [],
        "init_guide": "https://github.com/dfinity/bitcoin-canister/blob/master/e2e-tests/set_config.sh"
      }
    },
    "watchdog": {
      "type": "rust",
      "package": "watchdog",
      "candid": "./watchdog/candid.did"
    },
    "benchmarks": {
      "type": "rust",
      "candid": "./benchmarks/candid.did",
      "package": "benchmarks",
      "optimize": "cycles",
      "gzip": true
    },
    "upgradability-test": {
      "type": "custom",
      "candid": "./canister/candid.did",
      "wasm": "upgradability-test.wasm.gz"
    },
    "e2e-scenario-1": {
      "type": "rust",
      "candid": "./e2e-tests/scenario-1/candid.did",
      "package": "scenario-1",
      "optimize": "cycles",
      "gzip": true
    },
    "e2e-scenario-2": {
      "type": "rust",
      "candid": "./e2e-tests/scenario-2/candid.did",
      "package": "scenario-2",
      "optimize": "cycles",
      "gzip": true
    },
    "e2e-scenario-3": {
      "type": "rust",
      "candid": "./e2e-tests/scenario-3/candid.did",
      "package": "scenario-3",
      "optimize": "cycles",
      "gzip": true
    },
    "e2e-disable-api-if-not-fully-synced-flag": {
      "type": "rust",
      "candid": "./e2e-tests/disable-api-if-not-fully-synced-flag/candid.did",
      "package": "disable-api-if-not-fully-synced-flag",
      "optimize": "cycles",
      "gzip": true
    },
    "uploader": {
      "type": "rust",
      "candid": "./bootstrap/uploader/candid.did",
      "package": "uploader",
      "optimize": "cycles",
      "gzip": true
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral",
      "replica": {
        "subnet_type": "system"
      }
    }
  },
  "version": 1
}